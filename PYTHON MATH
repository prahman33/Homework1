1.#Mod Divmod
# Enter your code here. Read input from STDIN. Print output to STDOUT
def divMod(a,b):
    f = divmod(a,b)
    for e in f:
        print(e)
    print(f)
if __name__ == '__main__' :
    a = int(input())
    b = int(input())
    divMod(a,b)
    
    
    
2. #Power - Mod Power   
    # Enter your code here. Read input from STDIN. Print output to STDOUT
a, b, m = list(map(int, [input(), input(), input()]))

print(pow(a,b))
print(pow(a,b,m))



3.#Integers Come In All Sizes
# Enter your code here. Read input from STDIN. Print output to STDOUT
a, b, c, d = int(input()), int(input()), int(input()), int(input())
print(pow(a, b)+pow(c, d))




4.#Polar Coordinates
# Enter your code here. Read input from STDIN. Print output to STDOUT
import cmath
res = cmath.polar(complex(input()))
print(res[0])
print(res[1])
